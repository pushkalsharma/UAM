        <sys_updated_by>admin</sys_updated_by>
        <sys_updated_on>2018-06-26 14:46:55</sys_updated_on>
        <value>var appl = workflow.scratchpad.application;&#13;
var usr = current.variables.requested_for;&#13;
var list = current.variables.select_access.toString();&#13;
var flag = false;&#13;
var access_parent = '';&#13;
var listArr = list.split(',');&#13;
gs.addInfoMessage(listArr);&#13;
for (var key in listArr) {&#13;
	&#13;
	var par = new GlideRecord('x_14308_uam_user_a_user_to_application');&#13;
	par.addQuery('active', 'true');&#13;
	par.addQuery('user', usr);&#13;
	par.addQuery('application', appl);&#13;
	par.query();&#13;
	&#13;
	if(par.next())&#13;
		{&#13;
		access_parent = par.sys_id;&#13;
	}&#13;
	&#13;
	&#13;
	// Get the list of Access from the catalog&#13;
	var access = new GlideRecord('x_14308_uam_user_a_user_to_access');&#13;
	access.addQuery('user', usr);&#13;
	access.addQuery('application', access_parent);&#13;
	access.addQuery('access', listArr[key]);&#13;
	access.addQuery('active',true);&#13;
	access.query();&#13;
	&#13;
	// Check if an Active access already exist for the user, do nothing&#13;
	if(access.next())&#13;
		{&#13;
			flag = true;&#13;
	}&#13;
	&#13;
	else // if the access does not exist or is inactive, create a new record&#13;
		{&#13;
		var gr2 = new GlideRecord('x_14308_uam_user_a_user_to_access');&#13;
		gr2.initialize();&#13;
		gr2.access = listArr[key];&#13;
		gr2.user = usr;&#13;
		gr2.granted_ritm = current.sys_id;&#13;
		gr2.active = true;&#13;
		gr2.granted_date = new GlideDateTime();&#13;
		gr2.application = access_parent;&#13;
		gr2.insert();&#13;
			gs.addInfoMessage('entry to access done');&#13;
	}&#13;
}</value>
